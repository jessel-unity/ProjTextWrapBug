        </ui:VisualElement>
        <ui:VisualElement class="choice">
            <ui:Label text="Some Writing Tool 4" display-tooltip-when-elided="true" class="choice__label" />
            <ui:Label text="or similar" display-tooltip-when-elided="true" class="choice__secondary-label" />
        </ui:VisualElement>
        <ui:VisualElement class="choice">
            <ui:Label text="Some Writing Tool 5" display-tooltip-when-elided="true" class="choice__label" />
            <ui:Label text="or similar" display-tooltip-when-elided="true" class="choice__secondary-label" />
