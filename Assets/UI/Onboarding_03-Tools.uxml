    </ChoiceControlElement>
    <ChoiceControlElement class="container--stretch">
        <ui:Label text="Label 2" display-tooltip-when-elided="true" class="brand-label" />
        <ui:VisualElement class="choice">
            <ui:Label text="Some Fancy Art Tool 3" display-tooltip-when-elided="true" class="choice__label" />
            <ui:Label text="or similar" display-tooltip-when-elided="true" class="choice__secondary-label" />
        </ui:VisualElement>
        <ui:VisualElement class="choice">
            <ui:Label text="Some Fancy Art Tool 4" display-tooltip-when-elided="true" class="choice__label" />
            <ui:Label text="or similar" display-tooltip-when-elided="true" class="choice__secondary-label" />
        </ui:VisualElement>
    </ChoiceControlElement>
